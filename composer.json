{
  "name": "bertoost/craft-elasticemail",
  "description": "The Craft CMS implementation for the ElasticEmail email adapter",
  "type": "craft-plugin",
  "keywords": [
    "craft",
    "craftcms",
    "plugin",
    "craftcms plugin",
    "elastic email",
    "mailer",
    "adapter"
  ],
  "license": "MIT",
  "minimum-stability": "stable",
  "authors": [
    {
      "name": "Bert Oost",
      "email": "hello@bertoost.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "craftcms/cms": "^4.1|^5.0",
    "bertoost/elasticemail-mailer": "^v1.1.1"
  },
  "autoload": {
    "psr-4": {
      "bertoost\\ElasticEmail\\Craft\\": "src/"
    }
  },
  "extra": {
    "name": "ElasticEmail",
    "handle": "elastic-email",
    "hasCpSettings": false,
    "developer": "Bert Oost",
    "developerUrl": "https://bertoost.com",
    "documentationUrl": "https://github.com/bertoost/Craft-ElasticEmail/blob/master/README.md"
  }
}